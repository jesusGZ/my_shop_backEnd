{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug via inspector",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run-script", "start"],
			"port": 9229,
			"protocol": "inspector",
			"console": "integratedTerminal",
			"skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**"]
		}
	]
}
